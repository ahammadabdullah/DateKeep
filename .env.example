# Example .env file for DateKeep project

# URL for connecting to the PostgreSQL database
DATABASE_URL="postgresql://username:password@hostname:port/database?pgbouncer=true&connection_limit=1"

# Direct connection URL for database migrations
DIRECT_URL="postgresql://username:password@hostname:port/database"

# Telegram bot token for authentication
TELEGRAM_BOT_TOKEN="your-telegram-bot-token"

# URL of the web application
WEB_APP_URL="https://your-web-app-url"

# URL for the webhook endpoint
WEBHOOK_URL="https://your-web-app-url/api/bot"